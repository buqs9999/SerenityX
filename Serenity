pcall(function()
    game.StarterGui:SetCore("SendNotification", {
      Title = 'Thank You!';
      Text = 'thanks for using Doomware!';
      Duration = 20;
    })
    if not game.Players.LocalPlayer:IsInGroup(13135509) then
      coroutine.resume(coroutine.create(function()
         wait(10)
         game.StarterGui:SetCore("SendNotification", {
          Title = 'you should join the group';
          Text = 'the link to our Group was copied to your Clipboard!';
          Icon = "http://www.roblox.com/asset/?id=8801580862";
          Duration = 20;
         })
         setclipboard('https://www.roblox.com/groups/13135509/Zerg-Ent#!/about')
      end))
    end
  end)

